{
  "version": 0,
  "name": "Livestorm-Producthunt-Test",
  "alias": "stepoz-test-livestorm.now.sh",
  "routes": [
    {"src": "^/js/(.*)", "dest": "/js/$1"},
    {"src": "^/css/(.*)", "dest": "/css/$1"},
    {"src": "^/img/(.*)", "dest": "/img/$1"},
    {"src": "^/fonts/(.*)", "dest": "/fonts/$1"},
    {"src": "^/statics/(.*)", "dest": "/statics/$1"},
    {"src": ".*", "dest": "/index.html"}
  ]
}